pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    parameters {
        string(name: 'BRANCH', description: 'Branch to build')
    }
    stages {
        stage('Build') {
            steps {
                print 'Branch to build is ' + prams.BRANCH
                echo 'Building'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}